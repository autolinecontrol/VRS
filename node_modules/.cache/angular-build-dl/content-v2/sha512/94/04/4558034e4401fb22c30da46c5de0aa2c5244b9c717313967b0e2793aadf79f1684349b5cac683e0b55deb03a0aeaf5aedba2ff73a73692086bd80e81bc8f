          var identificacion = id.toString();
          this.db.collection('Usuarios', function (ref) {
            return ref.where('uid', '==', identificacion);
          }).get().subscribe(function (resultado) {
            resultado.empty;

            if (resultado.empty) {
              _this4.msj.mensajeError('Error', 'No se encontro el usuario');
            }

